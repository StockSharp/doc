{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj",
            "src/**.dll"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "topics/**.md",
          "topics/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "logo.png"
        ],
        "exclude": [
          "obj/**",
        ]
      }
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© StockSharp.</span>",
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "logo.png",
      "_enableNewTab": true
    },
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
        ]
      }
    ],
    "dest": "../../docfx_test/_site/en",
    "template": [
      "default",
      "../darkfx"
    ]
  },
  "__pdf": {
    "content": [
      {
        "files": [
          "api/**.yml",
        ],
        "exclude": [
          "**/toc.yml",
          "**/toc.md"
        ]
      },
      {
        "files": [
          "api/*.md",
          "topics/**.md",
          "topics/**.yml",
          "pdf/*"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
        ]
      }
    ],
    "wkhtmltopdf": {
      "additionalArguments": "--enable-local-file-access"
    },
    "dest": "../../docfx_test/_site/en"
  }
}