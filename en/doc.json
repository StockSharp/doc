{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj",
            "src/**.dll"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/*.yml" ]
      },
      {
        "files": [
          "api/*.md",
          "topics/**.md",
          "topics/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
        ],
        "exclude": [
          "obj/**",
        ]
      },
      {
        "files": ["favicon.ico","logo.svg"],
        "src": "../"
      }
    ],
    "xref": [
      "https://github.com/dotnet/docfx/raw/main/.xrefmap.json"
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© StockSharp.</span>",
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "logo.svg",
      "_enableNewTab": true
    },
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
        ]
      }
    ],
    "dest": "C:/inetpub/wwwroot/doc/en",
    "template": [
      "default",
      "modern"
    ],
    "sitemap": {
        "baseUrl": "https://doc.stocksharp.com",
        "priority": 0.1,
        "changefreq": "monthly",
        "fileOptions": {
            "**/api/**.yml": {
                "priority": 0.3,
                "lastmod": "2001-01-01",
            },
            "index.md": {
                "baseUrl": "https://doc.stocksharp.com",
                "priority": 0.8,
                "changefreq": "monthly"
            },
            "topics/**.md": {
                "baseUrl": "https://doc.stocksharp.com",
                "priority": 0.8,
                "changefreq": "monthly"
            }
        }
    }
  }
}